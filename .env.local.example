# SmartyStreets API Credentials
# Get your credentials at https://www.smarty.com/account/keys
# Client-side embedded key (for autocomplete)
# Create an embedded key with allowed hosts: localhost, 127.0.0.1
NEXT_PUBLIC_SMARTY_EMBEDDED_KEY=your_embedded_key_here

# Estated API (for property data)
# Get your token at https://estated.com/
ESTATED_API_TOKEN=your_estated_token_here
ESTATED_API_URL=https://apis.estated.com/v4/property

# Apify API (for city search listings)
# Get your token at https://apify.com/
APIFY_API_TOKEN=your_apify_token_here
APIFY_API_URL=https://api.apify.com/v2/acts/tri_angle~real-estate-aggregator/run-sync-get-dataset-items

# Firebase Configuration
# Get your config at https://console.firebase.google.com/
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id